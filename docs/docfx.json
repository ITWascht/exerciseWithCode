{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
  {
    "src": [
      {
        "src": "..",
        "files": [
          ".godot/mono/temp/bin/Debug/*.dll"
        ]
      }
    ],
    "dest": "api",
    "filter": "filterConfig.yml"
  }
],
  "build": {
    "content": [
      {
        "files": [
          "v1.0/**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "examples/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "mysite",
      "_appTitle": "mysite",
      "_appVersion": "v1.0",
      "_enableSearch": true,
      "pdf": true
    }
  }
}